{
  "include": ["src"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "jsxImportSource": "preact",
    "module": "ESNext",
    "target": "ESNext",
    "noEmit": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "*": [".snowpack/types/*"]
      // "*": [".snowpack/source/node_modules/*"]
    },
    "skipLibCheck": true,
    
  },
  // "exclude": [
  //   "node_modules"
  // ]
}